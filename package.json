{
  "name": "nexterm",
  "version": "1.2.0-BETA",
  "main": "server/index.js",
  "repository": "https://github.com/gnmyt/Nexterm.git",
  "author": "Mathias Wagner",
  "license": "MIT",
  "dependencies": {
    "@simplewebauthn/server": "^13.2.2",
    "archiver": "^7.0.1",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "decompress": "^4.2.1",
    "express": "^5.2.1",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^8.2.1",
    "express-ws": "^5.0.2",
    "flatbuffers": "^25.9.23",
    "joi": "^18.0.2",
    "js-yaml": "^4.1.1",
    "ldapts": "^8.1.6",
    "openid-client": "^6.8.2",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "webdav": "^5.9.0",
    "winston": "^3.19.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "esbuild": "^0.21.5",
    "nodemon": "^3.1.11",
    "vitepress": "^1.6.4",
    "vitepress-openapi": "^0.1.14"
  },
  "scripts": {
    "dev:server": "nodemon --ignore docs --ignore vendor --ignore client --ignore mobile --ignore data server/index.js",
    "dev:client": "cd client && yarn run dev",
    "dev:guacd": "./scripts/build-guacd.sh",
    "dev:engine": "./scripts/build-engine.sh",
    "dev": "concurrently --kill-others-on-fail \"yarn dev:server\" \"yarn dev:client -- --host\" \"yarn schema:watch\"",
    "docs:openapi": "node scripts/generate-openapi.js",
    "schema:generate": "flatc --ts --ts-flat-files -o server/lib/generated schema/control_plane.fbs schema/sftp_protocol.fbs && esbuild server/lib/generated/*_generated.ts --format=cjs --outdir=server/lib/generated --allow-overwrite --platform=node && rm -f server/lib/generated/*.ts",
    "schema:watch": "nodemon --watch schema -e fbs --exec 'yarn schema:generate'",
    "docs:dev": "yarn docs:openapi && vitepress dev docs",
    "docs:build": "yarn docs:openapi && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  }
}
